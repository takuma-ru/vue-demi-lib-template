{
  "name": "vue-demi-lib-template",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/**",
    "docs/**"
  ],
  "scripts": {
    "dev:ex2.6": "cd ./examples/nuxt2.6/ && yarn && yarn dev",
    "dev:ex3": "cd ./examples/vue3/ && yarn && yarn dev",

    "dev:2.7": "yarn workspace lib switch:2.7 && yarn workspace vue2.7 dev",
    "dev:nuxt2": "yarn workspace lib switch:2 && yarn workspace nuxt2.6 dev",
    "dev:3": "yarn workspace lib switch:3 && yarn workspace vue3 dev",
    "dev:docs": "yarn docs dev",

    "build:lib": "yarn workspace lib build",
    "build:docs": "yarn docs build",

    "publish:lib": "yarn workspace lib publish:lib",

    "vue2.7": "yarn workspace vue2.7",
    "nuxt2.6": "yarn workspace nuxt2.6",
    "vue3": "cd ",
    "lib": "yarn workspace lib",
    "docs": "yarn workspace docs"
  }
}